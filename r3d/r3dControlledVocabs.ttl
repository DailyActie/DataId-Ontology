	rdfs:label	"Repository Type vocabulary"@en ;
	skos:prefLabel	"Repository Type vocabulary"@en ;
	skos:altLabel	"TODO"@de	;
	skos:note	"TODO"@en	;
	skos:topConceptOf	:ControlledVocab	.
	
